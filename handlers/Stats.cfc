component output="false" singleton{	public void function index(event,rc,prc){		var PostbackService = getModel('PostbackService');		var LogService = getModel('LogService');		var LookupService = getModel('LookupService');		var StatsService = getModel('StatsService');				if( event.getvalue('test',false) ) {			var test = StatsService.getLogSpamHamStats();			writedump(var=test,top=9);			abort;		}		event.setView('stats/index');	}	public void function getLookupSpamStats(event,rc,prc) {		var StatsService = getModel('StatsService');		var data = StatsService.getLookupSpamStats();		event.renderData(data=data,type='json');
	}}